<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="kn_inherit_helpdesk_ticket_view_form" model="ir.ui.view">
        <field name="name">kn.helpdesk.ticket.form</field>
        <field name="model">helpdesk.ticket</field>
        <field name="inherit_id" ref="helpdesk.helpdesk_ticket_view_form"/>
        <field name="arch" type="xml">
             <xpath expr="//field[@name='team_id']" position="before">
                 <field name="so_id" options="{'no_create': True, 'no_edit': True}"/>
                 <field name="employee_id" options="{'no_create': True, 'no_edit': True}"/>

            </xpath>
               <xpath expr="//field[@name='partner_id']" position="before">
                   <field name="medium_id" options="{'no_create': True, 'no_edit': True}"/>
            </xpath>
            <xpath expr="//field[@name='partner_id']" position="after">
                   <field name="cust_gender" options="{'no_create': True, 'no_edit': True}"/>
            </xpath>
        </field>
    </record>
</odoo>